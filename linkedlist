class linkedList{
    constructor(head){
        this.head={
            value:head,
            next:null
        }
        this.length=1;
        this.tail=this.head;
    }
    addEnd(value){
        let Node={
            value:value,
            next:null
        }
        this.tail.next=Node;
        this.length+=1;
    }
    addStart(value){
       let Node={
            value:value,
            next:this.tail
        }
        this.head=Node;
        this.length+=1;
    }
    insertEle(index,value){
        let currentNode=this.head;
        let newNode=this.head;
        let i=0;
        for(let i=0;i<this.length;i++){
            // console.log()
            if(i==index){
                currentNode.value=value;
                currentNode.next=newNode;
                this.length+=1;
            }
            currentNode=currentNode.next;
            newNode=newNode.next;
        }
        // while(currentNode.next!=null){
            
        //     if(i==index){
        //         currentNode.value=value;
        //         currentNode.next=newNode;
        //     }
        //     i++;
        //     currentNode=currentNode.next;
        //     newNode=newNode.next;
        // }
        
    }

}
list=new linkedList(4);
list.addEnd(10);
list.addStart(6);
list.insertEle(1,56);
console.log(list)
